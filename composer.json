{
    "name": "shadowbane/cloudflare-dyndns",
    "description": "Dynamic DNS solution with cloudflare.",
    "keywords": ["laravel", "laravel zero", "console", "cli", "cloudflare", "dyndns"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/shadowbane/cloudflare-dyndns/issues",
        "source": "https://github.com/shadowbane/cloudflare-dyndns"
    },
    "authors": [
        {
            "name": "Adli I. Ifkar",
            "email": "adly.shadowbane@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/database": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/log": "^9.0",
        "illuminate/translation": "^9.37",
        "illuminate/validation": "^9.37",
        "laravel-zero/framework": "^9.2",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/termwind": "^1.14",
        "spatie/laravel-data": "^2.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.20",
        "intonate/tinker-zero": "^1.2",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.1",
        "spatie/laravel-ray": "^1.31"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["cfddns"]
}
